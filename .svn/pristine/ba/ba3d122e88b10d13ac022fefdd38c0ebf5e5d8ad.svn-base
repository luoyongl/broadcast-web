}

.holder::-webkit-input-placeholder {
	color: white;
}

.holder::-moz-placeholder {
	/* Mozilla Firefox 19+ */
	color: white;
}

.holder:-moz-placeholder {
	/* Mozilla Firefox 4 to 18 */
	color: white;
}

.holder:-ms-input-placeholder {
